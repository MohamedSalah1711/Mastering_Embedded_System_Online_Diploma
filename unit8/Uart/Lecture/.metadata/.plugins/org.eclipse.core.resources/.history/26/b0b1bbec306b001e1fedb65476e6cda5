	Enable_Clock();
	USART_Pin_Config USART_Config={Eight_Bit,Both,Baud_Rate_115200,Disable,Interrupt_Disable,none,NONE};
	USART_Config.P_CallBack= NULL;
	MCAL_USART_Init(USART1, &USART_Config);
	MCAL_USART_GPIO_Set_Pins(USART1);
	while(1){
		MCAL_USART_ReceiveData(USART1, &ch, Disable);
		MCAL_USART_Send_Data(USART1, &ch, Disable);

	}
}
