#include "Stm32f103x6.h"
#include "Stm32F103C6_EXTI_Driver.h"
#include "Stm32f103C6_Driver_USART.h"
#include "Stm32f103C6_Driver_gpio.h"
#include "Lcd.h"
void Enable_Clock(){
	ENABLE_CLOCK_GPIOA();
	ENABLE_CLOCK_GPIOB();
	ENABLE_CLOCK_AFIO();
}
uint8 ch;
int main(void)
{
	/*	USART_Stop_Bits   Stop;
	USART_Mode 		  Mode;
	USART_Baud_Rate	  Baud_Rate;
