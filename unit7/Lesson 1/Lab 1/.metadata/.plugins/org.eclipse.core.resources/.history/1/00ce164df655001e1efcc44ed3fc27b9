#ifndef REGISTERS_H_
#define REGISTERS_H_
//RCC reg
#define RCC_BASE 0x40021000
#define RCC_APB2ENR (*(volatile unsigned int *)(RCC_BASE + 0x18))

//GPIO reg
#define GPIO_Port_A_BASE 0x40010800
#define GPIO_Port_B_BASE 0x40010C00


#endif /* REGISTERS_H_ */
